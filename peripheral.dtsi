/ {
	watchdog1: ext_watchdog {
		/* ADM706 */
		compatible = "linux,wdt-gpio";
		gpios = <&gpio 28 GPIO_ACTIVE_LOW>;
		hw_algo = "toggle";
		hw_margin_ms = <400>;
	};

	gpio_keys{
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;
		//autorepeat;
#ifdef linux
#undef linux
#endif
		button@0 {
			label = "FRB";
			linux,code = <103>;
			linux,input-type;
			gpios = <&gpio 30 1>;
		};
		button1@1 {
			label = "MB";
			linux,code = <104>;
			linux,input-type;
			gpios = <&gpio 31 1>;
		};
	};

};

&i2c0 {
	status = "okay";

	stm32pmu@3a {
		compatible = "rau,stm32pmu";
		reg = <0x3a>;
	};
};

&i2c1 {
	status = "okay";

	eeprom@50 {
		compatible = "atmel,24c512";
		reg = <0x50>;
		pagesize = <256>;
	};
	eeprom@51 {
		compatible = "atmel,24c512";
